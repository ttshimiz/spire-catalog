  doi = {10.1111/j.1365-2966.2005.09238.x},
  eprint = {astro-ph/0411108},
  journal = {\mnras},
  keywords = {methods: numerical, galaxies: active, galaxies: interactions, galaxies: starburst, galaxies: structure},
  month = {aug},
  pages = {776-794},
  title = {{Modelling feedback from stars and black holes in galaxy mergers}},
  volume = {361},
  year = {2005},
  bdsk-file-1 = {},
  bdsk-url-1 = {http://dx.doi.org/10.1111/j.1365-2966.2005.09238.x},
