  arxivurl = {http://arXiv.org/abs/astro-ph/0301526},
  author = {{Cirasuolo}, M. and {Magliocchetti}, M. and {Celotti}, A. and {Danese}, L.},
  date-added = {2015-07-07 19:12:53 +0000},
