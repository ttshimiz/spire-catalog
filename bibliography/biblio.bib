  adsnote = {Provided by the SAO/NASA Astrophysics Data System},
  adsurl = {http://adsabs.harvard.edu/abs/2012ApJ...758...73S},
  archiveprefix = {arXiv},
  arxivurl = {http://arXiv.org/abs/1209.0476},
  author = {{Saintonge}, A. and {Tacconi}, L.~J. and {Fabello}, S. and {Wang}, J. and {Catinella}, B. and {Genzel}, R. and {Graci{\'a}-Carpio}, J. and {Kramer}, C. and {Moran}, S. and {Heckman}, T.~M. and {Schiminovich}, D. and {Schuster}, K. and {Wuyts}, S.},
  date-added = {2015-01-06 04:26:51 +0000},
