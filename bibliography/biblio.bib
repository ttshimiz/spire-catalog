  date-added = {2014-11-04 17:06:58 +0000},
  date-modified = {2014-11-04 17:07:01 +0000},
  doi = {10.1086/507766},
  eprint = {astro-ph/0609020},
  journal = {\apj},
  keywords = {Galaxies: Clusters: Individual: Name: Virgo, galaxies: individual (NGC 4569-M90), Galaxies: Interactions, Ultraviolet: Galaxies},
  month = {nov},
  pages = {811-821},
  title = {{The Fate of Spiral Galaxies in Clusters: The Star Formation History of the Anemic Virgo Cluster Galaxy NGC 4569}},
