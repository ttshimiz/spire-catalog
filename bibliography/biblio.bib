  doi = {10.1088/0004-637X/698/2/1550},
  eprint = {0809.3789},
  journal = {\apj},
  keywords = {cosmology: theory, galaxies: active, galaxies: evolution, quasars: general},
  month = {jun},
  pages = {1550-1569},
  title = {{Quasars Are Not Light Bulbs: Testing Models of Quasar Lifetimes with the Observed Eddington Ratio Distribution}},
  volume = {698},
  year = {2009},
