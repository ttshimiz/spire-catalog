  doi = {10.1088/0004-637X/752/2/87},
  eid = {87},
  eprint = {1204.2993},
  journal = {\apj},
  keywords = {accretion, accretion disks, galaxies: active, galaxies: nuclei, ISM: molecules, molecular processes},
  month = {jun},
