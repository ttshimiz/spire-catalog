@article{Xu:1999ty,
  abstract = {We have generated a sample of 409 active galactic nuclei (AGNs) for which both the radio luminosity at 5 GHz and the line luminosity in [O III] lambda5007 have been measured. The radio luminosity spans a range of 10 orders of magnitude, and the [O III] line luminosity spans a range of 8 orders of magnitude-both considerably larger than the ranges in previous studies. We show that these two quantities are correlated in a similar way for both radio-loud and radio-quiet AGNs. We demonstrate that the observed correlation can be explained in terms of a model in which jets are accelerated and collimated by a vertical magnetic field.},
