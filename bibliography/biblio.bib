  arxivurl = {http://arXiv.org/abs/astro-ph/9606178},
  author = {{Rush}, B. and {Malkan}, M.~A. and {Edelson}, R.~A.},
  date-added = {2015-07-07 19:55:22 +0000},
  date-modified = {2015-07-07 19:55:22 +0000},
  doi = {10.1086/178132},
  eprint = {astro-ph/9606178},
  journal = {\apj},
  keywords = {GALAXIES: ACTIVE, GALAXIES: SEYFERT, INFRARED: GALAXIES, RADIO CONTINUUM: GALAXIES, SURVEYS},
  month = {dec},
  pages = {130},
  title = {{The Radio Properties of Seyfert Galaxies in the 12 Micron and CfA Samples}},
