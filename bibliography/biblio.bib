  author = {{Gaibler}, V. and {Khochfar}, S. and {Krause}, M. and {Silk}, J.},
  date-added = {2014-05-02 19:08:05 +0000},
  date-modified = {2014-05-02 19:08:06 +0000},
  doi = {10.1111/j.1365-2966.2012.21479.x},
  eprint = {1111.4478},
  journal = {\mnras},
  keywords = {hydrodynamics, methods: numerical, ISM: clouds, galaxies: jets, galaxies: ISM, galaxies: star formation},
  month = {sep},
  pages = {438-449},
  primaryclass = {astro-ph.CO},
  title = {{Jet-induced star formation in gas-rich galaxies}},
  volume = {425},
