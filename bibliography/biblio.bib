  author = {{Schartmann}, M. and {Meisenheimer}, K. and {Camenzind}, M. and {Wolf}, S. and {Tristram}, K.~R.~W. and {Henning}, T.},
  date-added = {2013-09-24 14:51:15 +0000},
  date-modified = {2013-09-24 14:51:15 +0000},
  doi = {10.1051/0004-6361:20078907},
  eprint = {0802.2604},
