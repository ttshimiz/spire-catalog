  eprint = {astro-ph/0301526},
  journal = {\mnras},
  keywords = {galaxies: active, quasars: general, cosmology: observations},
  month = {may},
  pages = {993-1004},
  title = {{The radio-loud/radio-quiet dichotomy: news from the 2dF QSO Redshift Survey}},
