  date-modified = {2015-07-07 21:12:05 +0000},
  doi = {10.1086/313243},
  eprint = {astro-ph/9903313},
  journal = {\apjs},
  keywords = {GALAXIES: INDIVIDUAL: NAME: PICTOR A, GALAXIES: JETS, GALAXIES: KINEMATICS AND DYNAMICS, GALAXIES: NUCLEI, GALAXIES: STRUCTURE, RADIO CONTINUUM: GALAXIES, Galaxies: Individual: Name: Pictor A, Galaxies: Jets, Galaxies: Kinematics and Dynamics, Galaxies: Nuclei, Galaxies: Structure, Radio Continuum: Galaxies},
  month = {aug},
