  author = {{Tombesi}, F. and {Cappi}, M. and {Reeves}, J.~N. and {Palumbo}, G.~G.~C. and {Yaqoob}, T. and {Braito}, V. and {Dadina}, M.},
  date-added = {2014-03-12 15:31:02 +0000},
