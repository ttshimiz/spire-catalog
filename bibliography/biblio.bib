  author = {{Brinchmann}, J. and {Charlot}, S. and {White}, S.~D.~M. and {Tremonti}, C. and {Kauffmann}, G. and {Heckman}, T. and {Brinkmann}, J.},
  date-added = {2014-03-17 20:43:48 +0000},
