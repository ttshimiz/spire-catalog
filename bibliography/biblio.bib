  date-added = {2015-07-13 16:32:55 +0000},
  date-modified = {2015-07-13 16:32:55 +0000},
  doi = {10.1093/mnras/stu1841},
  eprint = {1409.1815},
  journal = {\mnras},
  keywords = {galaxies: ISM, galaxies: spiral, infrared: galaxies},
  month = {mar},
  pages = {135-167},
  title = {{The identification of dust heating mechanisms in nearby galaxies using Herschel 160/250 and 250/350 {$\mu$}m surface brightness ratios}},
  volume = {448},
