  doi = {10.1051/0004-6361/201118200},
  eid = {A57},
  eprint = {1204.3162},
  journal = {\aap},
  keywords = {galaxies: nuclei, radio continuum: galaxies, techniques: interferometric},
  month = {jul},
  pages = {A57},
  primaryclass = {astro-ph.CO},
  title = {{Compact radio emission from z \~{} 0.2 X-ray bright AGN}},
