  author = {{Mendez}, A.~J. and {Coil}, A.~L. and {Aird}, J. and {Diamond-Stanic}, A.~M. and {Moustakas}, J. and {Blanton}, M.~R. and {Cool}, R.~J. and {Eisenstein}, D.~J. and {Wong}, K.~C. and {Zhu}, G.},
  date-added = {2013-02-14 17:11:47 +0000},
  date-modified = {2013-02-14 17:11:48 +0000},
  eprint = {1302.2920},
  journal = {ArXiv e-prints},
  keywords = {Astrophysics - Cosmology and Extragalactic Astrophysics},
  month = {feb},
  primaryclass = {astro-ph.CO},
  title = {{PRIMUS: Infrared and X-ray AGN Selection Techniques at 0.2{\lt}z{\lt}1.2}},
