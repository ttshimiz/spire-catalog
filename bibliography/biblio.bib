  date-added = {2013-01-30 15:47:57 +0000},
  date-modified = {2013-01-30 15:47:57 +0000},
  doi = {10.1111/j.1365-2966.2011.19002.x},
  eprint = {1008.1583},
  journal = {\mnras},
  keywords = {galaxies: active, galaxies: elliptical and lenticular, cD, galaxies: evolution, galaxies: interactions, galaxies: starburst},
  month = {aug},
  pages = {3798-3806},
  primaryclass = {astro-ph.CO},
  title = {{A simple model for AGN feedback in nearby early-type galaxies}},
