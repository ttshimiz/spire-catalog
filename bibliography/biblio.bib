  author = {{Fabello}, S. and {Kauffmann}, G. and {Catinella}, B. and {Giovanelli}, R. and {Haynes}, M.~P. and {Heckman}, T.~M. and {Schiminovich}, D.},
  date-added = {2015-01-06 04:46:24 +0000},
  date-modified = {2015-01-06 04:46:24 +0000},
  doi = {10.1111/j.1365-2966.2011.18825.x},
  journal = {\mnras},
  keywords = {galaxies: active, galaxies: evolution, radio lines: galaxies},
  month = {sep},
  pages = {1739-1744},
  title = {{Arecibo Legacy Fast ALFA H I data stacking - II. H I content of the host galaxies of active galactic nuclei}},
