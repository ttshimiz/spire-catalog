  doi = {10.1046/j.1365-2966.2003.07105.x},
  eprint = {astro-ph/0306415},
  journal = {\mnras},
  keywords = {galaxies: active, cosmology: observations, radio continuum: general},
  month = {dec},
  pages = {447-455},
  title = {{Is there a dichotomy in the radio loudness distribution of quasars?}},
  volume = {346},
