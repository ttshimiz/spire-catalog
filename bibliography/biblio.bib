  arxivurl = {http://arXiv.org/abs/astro-ph/0303617},
  author = {{Alonso-Herrero}, A. and {Quillen}, A.~C. and {Rieke}, G.~H. and {Ivanov}, V.~D. and {Efstathiou}, A.},
