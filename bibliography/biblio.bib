}


@article{Hanish:2013rp,
  abstract = {We use the Spitzer Space Telescope Enhanced Imaging Products and the Spitzer Archival Far-InfraRed Extragalactic Survey to study the spectral energy distributions (SEDs) of spectroscopically confirmed type 1 quasars selected from the Sloan Digital Sky Survey (SDSS). By combining the Spitzer and SDSS data with the Two Micron All Sky Survey, we are able to construct a statistically robust rest-frame 0.1-100 mum type 1 quasar template. We find that the quasar population is well-described by a single power-law SED at wavelengths less than 20 mum, in good agreement with previous work. However, at longer wavelengths, we find a significant excess in infrared luminosity above an extrapolated power-law, along with significant object-to-object dispersion in the SED. The mean excess reaches a maximum of 0.8 dex at rest-frame wavelengths near 100 mum.},
