  date-added = {2014-03-12 15:29:27 +0000},
  date-modified = {2014-03-12 15:29:28 +0000},
  doi = {10.1038/nature06319},
  eprint = {0802.4024},
  journal = {\nat},
  month = {nov},
  pages = {74-76},
  title = {{The rotating wind of the quasar PG 1700+518}},
  volume = {450},
  year = {2007},
  bdsk-file-1 = {},
  bdsk-url-1 = {http://dx.doi.org/10.1038/nature06319},
