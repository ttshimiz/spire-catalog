  author = {{Hern{\'a}n-Caballero}, A. and {Alonso-Herrero}, A. and {Hatziminaoglou}, E. and {Spoon}, H.~W.~W. and {Ramos Almeida}, C. and {D{\'{\i}}az Santos}, T. and {H{\"o}nig}, S.~F. and {Gonz{\'a}lez-Mart{\'{\i}}n}, O. and {Esquej}, P.},
  date-added = {2015-02-24 18:00:51 +0000},
  date-modified = {2015-02-24 18:00:54 +0000},
