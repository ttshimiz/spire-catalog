  eprint = {astro-ph/0209607},
  journal = {\apj},
  keywords = {Accretion, Accretion Disks, Galaxies: Active, Galaxies: Nuclei, Radio Continuum: Galaxies, X-Rays: Galaxies},
  month = {jan},
