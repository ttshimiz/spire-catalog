  archiveprefix = {arXiv},
  arxivurl = {http://arXiv.org/abs/1501.07664},
  author = {{Tombesi}, F. and {Mel{\'e}ndez}, M. and {Veilleux}, S. and {Reeves}, J.~N. and {Gonz{\'a}lez-Alfonso}, E. and {Reynolds}, C.~S.},
  date-added = {2015-04-24 15:46:50 +0000},
  date-modified = {2015-04-24 15:46:50 +0000},
  doi = {10.1038/nature14261},
  eprint = {1501.07664},
  journal = {\nat},
  month = {mar},
  pages = {436-438},
  primaryclass = {astro-ph.HE},
  title = {{Wind from the black-hole accretion disk driving a molecular outflow in an active galaxy}},
  volume = {519},
