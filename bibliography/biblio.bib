  date-added = {2014-03-25 14:57:48 +0000},
  date-modified = {2014-03-25 14:57:49 +0000},
  doi = {10.1051/0004-6361/200912497},
  eprint = {0909.5439},
  journal = {\aap},
  keywords = {methods: data analysis, galaxies: fundamental parameters, galaxies: stellar content, galaxies: ISM, ultraviolet: galaxies, infrared: galaxies},
