  archiveprefix = {arXiv},
  arxivurl = {http://arXiv.org/abs/1304.5231},
  author = {{Shipley}, H.~V. and {Papovich}, C. and {Rieke}, G.~H. and {Dey}, A. and {Jannuzi}, B.~T. and {Moustakas}, J. and {Weiner}, B.},
  date-added = {2014-05-02 15:43:11 +0000},
  date-modified = {2014-05-02 15:43:11 +0000},
