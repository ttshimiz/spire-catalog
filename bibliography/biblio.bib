  date-added = {2014-03-24 20:28:34 +0000},
  date-modified = {2014-03-24 20:28:34 +0000},
  doi = {10.1111/j.1365-2966.2009.14455.x},
  eprint = {0812.2476},
  journal = {\mnras},
  keywords = {galaxies: formation , galaxies: high-redshift , galaxies: starburst , infrared: galaxies , radio continuum: galaxies , submillimetre},
  month = {apr},
