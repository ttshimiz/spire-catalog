  keywords = {galaxies: active, galaxies: starburst, infrared: galaxies, radio continuum: galaxies, stars: formation},
  month = {may},
  pages = {2},
  primaryclass = {astro-ph.CO},
  title = {{Radio and Mid-infrared Properties of Compact Starbursts: Distancing Themselves from the Main Sequence}},
