@article{Draine:2012vf,
  abstract = {The Small Magellanic Cloud (SMC) has surprisingly strong submillimeter- and millimeter-wavelength emission that is inconsistent with standard dust models, including those with emission from spinning dust. Here, we show that the emission from the SMC may be understood if the interstellar dust mixture includes magnetic nanoparticles, emitting magnetic dipole radiation resulting from thermal fluctuations in the magnetization. The magnetic grains can be metallic iron, magnetite Fe3O4, or maghemite gamma-Fe2O3. The required mass of iron is consistent with elemental abundance constraints. The magnetic dipole emission is predicted to be polarized orthogonally to the normal electric dipole radiation if the nanoparticles are inclusions in larger grains. We speculate that other low-metallicity galaxies may also have a large fraction of the interstellar Fe in magnetic materials.},
  adsnote = {Provided by the SAO/NASA Astrophysics Data System},
  adsurl = {http://adsabs.harvard.edu/abs/2012ApJ...757..103D},
  archiveprefix = {arXiv},
  arxivurl = {http://arXiv.org/abs/1205.6810},
  author = {{Draine}, B.~T. and {Hensley}, B.},
  date-added = {2015-07-14 16:54:02 +0000},
  date-modified = {2015-07-14 16:54:02 +0000},
  doi = {10.1088/0004-637X/757/1/103},
  eid = {103},
  eprint = {1205.6810},
  journal = {\apj},
  keywords = {dust, extinction, infrared: galaxies, infrared: ISM, Magellanic Clouds, polarization, radiation mechanisms: thermal, radio continuum: ISM},
  month = {sep},
  pages = {103},
  title = {{The Submillimeter and Millimeter Excess of the Small Magellanic Cloud: Magnetic Dipole Emission from Magnetic Nanoparticles?}},
  volume = {757},
  year = {2012},
  bdsk-file-1 = {},
