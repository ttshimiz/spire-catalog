  date-added = {2013-09-24 15:07:27 +0000},
  date-modified = {2013-09-24 15:07:27 +0000},
  doi = {10.1086/590482},
  eprint = {0806.0511},
  journal = {\apj},
  keywords = {ISM: Dust, Extinction, Galaxies: Active, Galaxies: Seyfert, Infrared: General, Galaxies: Quasars: General, Radiative Transfer},
  month = {sep},
  pages = {147-159},
  title = {{AGN Dusty Tori. I. Handling of Clumpy Media}},
  volume = {685},
