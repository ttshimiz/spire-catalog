  journal = {\apj},
  keywords = {diffuse radiation, galaxies: active, quasars: general, surveys, X-rays: diffuse background},
  month = {may},
  pages = {104},
  title = {{Toward the Standard Population Synthesis Model of the X-Ray Background: Evolution of X-Ray Luminosity and Absorption Functions of Active Galactic Nuclei Including Compton-thick Populations}},
  volume = {786},
  year = {2014},
  bdsk-file-1 = {},
  bdsk-url-1 = {http://dx.doi.org/10.1088/0004-637X/786/2/104},
