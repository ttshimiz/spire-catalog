  author = {{Toba}, Y. and {Oyabu}, S. and {Matsuhara}, H. and {Malkan}, M.~A. and {Gandhi}, P. and {Nakagawa}, T. and {Isobe}, N. and {Shirahata}, M. and {Oi}, N. and {Ohyama}, Y. and {Takita}, S. and {Yamauchi}, C. and {Yano}, K.},
  date-added = {2014-06-23 14:58:27 +0000},
  date-modified = {2014-06-23 14:58:28 +0000},
  doi = {10.1088/0004-637X/788/1/45},
  eid = {45},
  journal = {\apj},
  keywords = {catalogs, galaxies: active, galaxies: luminosity function, mass function, galaxies: nuclei, infrared: galaxies, methods: statistical},
  month = {jun},
