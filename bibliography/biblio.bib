  date-added = {2015-07-14 19:46:08 +0000},
  date-modified = {2015-07-14 19:46:08 +0000},
  doi = {10.1093/mnras/stv988},
  eprint = {1504.01226},
  journal = {\mnras},
  keywords = {galaxies: active, galaxies: jets, galaxies: nuclei, radio continuum: galaxies, X-rays: galaxies},
  month = {jul},
  pages = {5036-5045},
  title = {{Discovery of millimetre-wave excess emission in radio-quiet active galactic nuclei}},
