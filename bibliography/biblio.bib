  date-added = {2013-09-01 15:37:52 +0000},
  date-modified = {2013-09-01 15:37:52 +0000},
  doi = {10.1086/591048},
  eprint = {0806.2863},
  journal = {\apj},
  keywords = {Galaxies: Active, Galaxies: Evolution, Galaxies: Formation, Galaxies: Seyfert, Galaxies: Starburst, X-Rays: Diffuse Background},
  month = {oct},
  pages = {787-800},
