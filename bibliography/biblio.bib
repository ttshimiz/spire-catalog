@article{Trump:2012ly,
  abstract = {We present some of the first science data with the new Keck/MOSFIRE instrument to test the effectiveness of different AGN/SF diagnostics at z~1.5. MOSFIRE spectra were obtained in three H-band multi-slit masks in the GOODS-S field, resulting in two hour exposures of 36 emission-line galaxies. We compare X-ray data with the traditional emission-line ratio diagnostics and the alternative mass-excitation and color-excitation diagrams, combining new MOSFIRE infrared data with previous HST/WFC3 infrared spectra (from the 3D-HST survey) and multiwavelength photometry. We demonstrate that a high [OIII]/Hb ratio is insufficient as an AGN indicator at z>1. For the four X-ray detected galaxies, the classic diagnostics ([OIII]/Hb vs. [NII]/Ha and [SII]/Ha) remain consistent with X-ray AGN/SF classification. The X-ray data also suggest that composite galaxies (with intermediate AGN/SF classification) host bona-fide AGNs. Nearly 2/3 of the z~1.5 emission-line galaxies have nuclear activity detected by either X-rays or the classic diagnostics. Compared to the X-ray and line ratio classifications, the mass-excitation method remains effective at z>1, but we show that the color-excitation method requires a new calibration to successfully identify AGNs at these redshifts.},
  adsnote = {Provided by the SAO/NASA Astrophysics Data System},
  adsurl = {http://adsabs.harvard.edu/abs/2012arXiv1212.2218T},
  archiveprefix = {arXiv},
  author = {{Trump}, J.~R. and {Konidaris}, N.~P. and {Barro}, G. and {Koo}, D.~C. and {Kocevski}, D.~D. and {Juneau}, S. and {Weiner}, B.~J. and {Faber}, S.~M. and {McLean}, I.~S. and {Yan}, R. and {Perez-Gonzalez}, P.~G. and {Villar}, V.},
  date-added = {2012-12-16 17:15:53 +0000},
  date-modified = {2012-12-16 17:15:53 +0000},
  eprint = {1212.2218},
  journal = {ArXiv e-prints},
  keywords = {Astrophysics - Cosmology and Extragalactic Astrophysics},
  month = {dec},
