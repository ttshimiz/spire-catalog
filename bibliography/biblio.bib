  date-added = {2014-03-21 18:12:08 +0000},
  date-modified = {2014-03-21 18:12:09 +0000},
  doi = {10.1088/0004-637X/778/2/94},
  eid = {94},
  eprint = {1309.3559},
  journal = {\apj},
  keywords = {galaxies: active, galaxies: photometry, galaxies: Seyfert, galaxies: star formation, galaxies: statistics, infrared: galaxies, radio continuum: galaxies, surveys},
  month = {dec},
  pages = {94},
  primaryclass = {astro-ph.CO},
  title = {{The Mid-infrared Emission of Narrow-line Active Galactic Nuclei: Star Formation, Nuclear Activity, and Two Populations Revealed by Wise}},
  volume = {778},
  year = {2013},
  bdsk-file-1 = {},
