  eprint = {1306.2488},
  journal = {Journal of Physical Chemistry A},
  keywords = {astrochemistry, galaxies: active, galaxies: starburst, interstellar medium},
  month = {oct},
  pages = {9593-9604},
  primaryclass = {astro-ph.GA},
