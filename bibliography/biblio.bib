  author = {{Young}, J.~E. and {Eracleous}, M. and {Shemmer}, O. and {Netzer}, H. and {Gronwall}, C. and {Lutz}, D. and {Ciardullo}, R. and {Sturm}, E.},
  date-added = {2013-11-21 17:51:00 +0000},
  date-modified = {2013-11-21 17:51:01 +0000},
  eprint = {1311.1978},
  journal = {ArXiv e-prints},
  keywords = {Astrophysics - Cosmology and Extragalactic Astrophysics},
  month = {nov},
  primaryclass = {astro-ph.CO},
  title = {{Locating Star-Forming Regions in Quasar Host Galaxies}},
  year = {2013},
