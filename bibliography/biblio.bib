  eid = {A57},
  eprint = {1006.2858},
  journal = {\aap},
  keywords = {black hole physics, X-ray: galaxies, galaxies: Seyfert, line: identification},
  month = {oct},
  pages = {A57},
  primaryclass = {astro-ph.HE},
  title = {{Evidence for ultra-fast outflows in radio-quiet AGNs. I. Detection and statistical incidence of Fe K-shell absorption lines}},
